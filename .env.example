# Environment variables for Zoho People Dashboard

# Zoho People API Configuration
ZOHO_CLIENT_ID=your_client_id_here
ZOHO_CLIENT_SECRET=your_client_secret_here

# Production URL (for live deployment)
ZOHO_REDIRECT_URI=https://zohopeople.netlify.app/auth/callback

# Development URL (for local testing)
# ZOHO_REDIRECT_URI=http://localhost:5173/auth/callback

ZOHO_ACCESS_TOKEN=
ZOHO_REFRESH_TOKEN=
ZOHO_DOMAIN=people.zoho.in

# Environment
NODE_ENV=production
